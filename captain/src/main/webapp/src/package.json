{
  "name": "captain",
  "version": "1.0.0",
  "description": "Anicloud Development Project",
  "main": "app/index.jsx",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --progress --profile --hot",
    "build": "./node_modules/.bin/webpack --progress --colors --config webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint app",
    "clean": "rm -rf build/*",
    "test": "./node_modules/.bin/mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "dev": {
      "command": "./node_modules/.bin/webpack-dev-server --progress",
      "env": {
        "BUILD_DEV": "development"
      }
    },
    "build": {
      "env": {
        "BUILD_RELEASE": "release"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anicloud/captain.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "es6",
    "babel"
  ],
  "author": "fantasybh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anicloud/captain/issues"
  },
  "homepage": "https://github.com/anicloud/captain#readme",
  "dependencies": {
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.21.0",
    "open-browser-webpack-plugin": "0.0.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
