{
  "name": "captain",
  "version": "1.0.0",
  "description": "Anicloud Development Project",
  "main": "app/index.jsx",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --colors --config webpack.prod.config.js",
    "lint": "eslint app",
    "clean": "rm -rf ./public/*",
    "test": "mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm run test -- --watch"
  },
  "betterScripts": {
    "dev": {
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anicloud/captain.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "es6",
    "babel"
  ],
  "author": "fantasybh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anicloud/captain/issues"
  },
  "homepage": "https://github.com/anicloud/captain#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-icons": "^2.1.0",
    "react-redux": "^4.4.5",
    "react-responsive": "^1.1.4",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reqwest": "^2.0.5"
  },
